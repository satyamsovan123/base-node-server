{
  "plugins": [],
  "recurseDepth": 10,
  "opts": {
    "template": "assets/template/docdash/",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "source": {
    "include": ["app", "configs", "utils", "constants", "app.js", "README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Global",
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "meta": {
      "title": "base-node-server",
      "description": "base-node-server",
      "keyword": "base-server"
    },
    "search": false,
    "commonNav": false,
    "collapse": false,
    "wrap": true,
    "typedefs": true,
    "private": true,
    "removeQuotes": false,
    "scripts": [],
    "ShortenTypes": false,
    "menu": {
      "Project Website": {
        "href": "https://github.com/satyamsovan123/base-node-server/tree/main/backend",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    },
    "scopeInOutputPath": true,
    "nameInOutputPath": true,
    "versionInOutputPath": true
  }
}
